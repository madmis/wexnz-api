{
    "name": "madmis/btce-api",
    "description": "Btc-e.com REST API php client",
    "keywords": [
        "btc-e.com",
        "BTCE",
        "rest",
        "api"
    ],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^7.1",
        "guzzlehttp/psr7": "^1.4",
        "guzzlehttp/guzzle": "^6.3",
        "doctrine/annotations": "^1.4",
        "symfony/options-resolver": "^3.3",
        "symfony/serializer": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "madmis\\": "src",
            "Test\\": "tests"
        }
    },
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "^6.2",
        "symfony/console": "^3.3"
    }
}
